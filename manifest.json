{
  "name": "Webigation",
  "description": "Capstone project",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_title": "Webigation"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "nativeMessaging",
    "scripting",
    "activeTab",
    "storage",
    "notifications"
  ],
  "host_permissions": ["http://*/", "https://*/"],
  "commands": {
    "pre": {
      "suggested_key": {
        "default": "Ctrl+Left"
      },
      "description": "pre keyboard short cut",
      "global": true
    },
    "next": {
      "suggested_key": {
        "default": "Ctrl+Right"
      },
      "description": "next keyboard short cut",
      "global": true
    }
  }
}
