{
  "name": "New Chrome Extension",
  "description": "Capstone project",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup" : "popup.html"

  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./contentScripts/script.js"],
      "css": ["./contentScripts/style.css"],
      "run_at": "document_end"
    }
    
  ]
  
}
